/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": false,
    "declaration": false,
    "module": "ES2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "target": "es5",
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts",
    "dist",
    "node_modules"
  ]
}
